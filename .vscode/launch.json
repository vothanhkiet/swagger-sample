    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "protocol": "inspector",
            "console": "integratedTerminal",
            "runtimeExecutable": "${workspaceFolder}/.vscode/runtime.sh",
            "cwd": "${workspaceRoot}/src",
            "name": "Debug Node",
            "program": "${workspaceFolder}/src/application.js",
            "env": {
                "NODE_ENV": "development",
                "TZ": "UTC"
            }
        }
    ]
}
